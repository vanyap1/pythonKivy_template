/dts-v1/;

/ {
	interrupt-parent = <0x01>;
	#address-cells = <0x01>;
	#size-cells = <0x01>;
	model = "FriendlyARM NanoPi Duo2";
	compatible = "friendlyarm,nanopi-duo2\0allwinner,sun8i-h3";

	chosen {
		#address-cells = <0x01>;
		#size-cells = <0x01>;
		ranges;
		stdout-path = "serial0:115200n8";

		framebuffer-hdmi {
			compatible = "allwinner,simple-framebuffer\0simple-framebuffer";
			allwinner,pipeline = "mixer0-lcd0-hdmi";
			clocks = <0x02 0x06 0x03 0x66 0x03 0x6f>;
			status = "disabled";
		};

		framebuffer-tve {
			compatible = "allwinner,simple-framebuffer\0simple-framebuffer";
			allwinner,pipeline = "mixer1-lcd1-tve";
			clocks = <0x02 0x07 0x03 0x67>;
			status = "disabled";
		};
	};

	clocks {
		#address-cells = <0x01>;
		#size-cells = <0x01>;
		ranges;

		osc24M_clk {
			#clock-cells = <0x00>;
			compatible = "fixed-clock";
			clock-frequency = <0x16e3600>;
			clock-accuracy = <0xc350>;
			clock-output-names = "osc24M";
			phandle = <0x1b>;
		};

		osc32k_clk {
			#clock-cells = <0x00>;
			compatible = "fixed-clock";
			clock-frequency = <0x8000>;
			clock-accuracy = <0xc350>;
			clock-output-names = "ext_osc32k";
			phandle = <0x2e>;
		};
	};

	display-engine {
		compatible = "allwinner,sun8i-h3-display-engine";
		allwinner,pipelines = <0x04 0x05>;
		status = "disabled";
		phandle = <0x46>;
	};

	sound_hdmi {
		compatible = "allwinner,sun9i-a80-hdmi-audio\0allwinner,sun8i-h3-hdmi-audio";
		status = "disabled";
		phandle = <0x47>;

		codec {
			sound-dai = <0x06>;
		};

		cpu {
			sound-dai = <0x07>;
		};
	};

	scpi {
		compatible = "arm,scpi";
		mboxes = <0x08 0x02 0x08 0x03>;
		mbox-names = "tx\0rx";
		shmem = <0x09>;
		phandle = <0x48>;
	};

	soc {
		compatible = "simple-bus";
		#address-cells = <0x01>;
		#size-cells = <0x01>;
		dma-ranges;
		ranges;

		clock@1000000 {
			reg = <0x1000000 0x10000>;
			clocks = <0x03 0x30 0x03 0x65>;
			clock-names = "bus\0mod";
			resets = <0x03 0x22>;
			#clock-cells = <0x01>;
			#reset-cells = <0x01>;
			compatible = "allwinner,sun8i-h3-de2-clk";
			phandle = <0x02>;
		};

		mixer@1100000 {
			compatible = "allwinner,sun8i-h3-de2-mixer-0";
			reg = <0x1100000 0x100000>;
			clocks = <0x02 0x00 0x02 0x06>;
			clock-names = "bus\0mod";
			resets = <0x02 0x00>;
			phandle = <0x04>;

			ports {
				#address-cells = <0x01>;
				#size-cells = <0x00>;

				port@1 {
					#address-cells = <0x01>;
					#size-cells = <0x00>;
					reg = <0x01>;
					phandle = <0x49>;

					endpoint@0 {
						reg = <0x00>;
						remote-endpoint = <0x0a>;
						phandle = <0x0e>;
					};

					endpoint@1 {
						reg = <0x01>;
						remote-endpoint = <0x0b>;
						phandle = <0x11>;
					};
				};
			};
		};

		mixer@1200000 {
			compatible = "allwinner,sun8i-h3-de2-mixer-1";
			reg = <0x1200000 0x100000>;
			clocks = <0x02 0x01 0x02 0x07>;
			clock-names = "bus\0mod";
			resets = <0x02 0x02>;
			phandle = <0x05>;

			ports {
				#address-cells = <0x01>;
				#size-cells = <0x00>;

				port@1 {
					#address-cells = <0x01>;
					#size-cells = <0x00>;
					reg = <0x01>;
					phandle = <0x4a>;

					endpoint@0 {
						reg = <0x00>;
						remote-endpoint = <0x0c>;
						phandle = <0x0f>;
					};

					endpoint@1 {
						reg = <0x01>;
						remote-endpoint = <0x0d>;
						phandle = <0x12>;
					};
				};
			};
		};

		dma-controller@1c02000 {
			compatible = "allwinner,sun8i-h3-dma";
			reg = <0x1c02000 0x1000>;
			interrupts = <0x00 0x32 0x04>;
			clocks = <0x03 0x15>;
			resets = <0x03 0x06>;
			#dma-cells = <0x01>;
			phandle = <0x21>;
		};

		lcd-controller@1c0c000 {
			compatible = "allwinner,sun8i-h3-tcon-tv\0allwinner,sun8i-a83t-tcon-tv";
			reg = <0x1c0c000 0x1000>;
			interrupts = <0x00 0x56 0x04>;
			clocks = <0x03 0x2a 0x03 0x66>;
			clock-names = "ahb\0tcon-ch1";
			resets = <0x03 0x1b>;
			reset-names = "lcd";
			phandle = <0x4b>;

			ports {
				#address-cells = <0x01>;
				#size-cells = <0x00>;

				port@0 {
					#address-cells = <0x01>;
					#size-cells = <0x00>;
					reg = <0x00>;
					phandle = <0x4c>;

					endpoint@0 {
						reg = <0x00>;
						remote-endpoint = <0x0e>;
						phandle = <0x0a>;
					};

					endpoint@1 {
						reg = <0x01>;
						remote-endpoint = <0x0f>;
						phandle = <0x0c>;
					};
				};

				port@1 {
					#address-cells = <0x01>;
					#size-cells = <0x00>;
					reg = <0x01>;
					phandle = <0x4d>;

					endpoint@1 {
						reg = <0x01>;
						remote-endpoint = <0x10>;
						phandle = <0x2d>;
					};
				};
			};
		};

		lcd-controller@1c0d000 {
			compatible = "allwinner,sun8i-h3-tcon-tv\0allwinner,sun8i-a83t-tcon-tv";
			reg = <0x1c0d000 0x1000>;
			interrupts = <0x00 0x57 0x04>;
			clocks = <0x03 0x2b 0x03 0x67>;
			clock-names = "ahb\0tcon-ch1";
			resets = <0x03 0x1c>;
			reset-names = "lcd";
			phandle = <0x4e>;

			ports {
				#address-cells = <0x01>;
				#size-cells = <0x00>;

				port@0 {
					#address-cells = <0x01>;
					#size-cells = <0x00>;
					reg = <0x00>;
					phandle = <0x4f>;

					endpoint@0 {
						reg = <0x00>;
						remote-endpoint = <0x11>;
						phandle = <0x0b>;
					};

					endpoint@1 {
						reg = <0x01>;
						remote-endpoint = <0x12>;
						phandle = <0x0d>;
					};
				};

				port@1 {
					#address-cells = <0x01>;
					#size-cells = <0x00>;
					reg = <0x01>;
					phandle = <0x50>;

					endpoint@1 {
						reg = <0x01>;
						remote-endpoint = <0x13>;
						phandle = <0x37>;
					};
				};
			};
		};

		mmc@1c0f000 {
			reg = <0x1c0f000 0x1000>;
			pinctrl-names = "default";
			pinctrl-0 = <0x14>;
			resets = <0x03 0x07>;
			reset-names = "ahb";
			interrupts = <0x00 0x3c 0x04>;
			bus-width = <0x04>;
			status = "okay";
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			compatible = "allwinner,sun7i-a20-mmc";
			clocks = <0x03 0x16 0x03 0x47 0x03 0x49 0x03 0x48>;
			clock-names = "ahb\0mmc\0output\0sample";
			cd-gpios = <0x15 0x05 0x06 0x01>;
			vmmc-supply = <0x16>;
			phandle = <0x51>;
		};

		mmc@1c10000 {
			reg = <0x1c10000 0x1000>;
			pinctrl-names = "default";
			pinctrl-0 = <0x17>;
			resets = <0x03 0x08>;
			reset-names = "ahb";
			interrupts = <0x00 0x3d 0x04>;
			status = "okay";
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			compatible = "allwinner,sun7i-a20-mmc";
			clocks = <0x03 0x17 0x03 0x4a 0x03 0x4c 0x03 0x4b>;
			clock-names = "ahb\0mmc\0output\0sample";
			vmmc-supply = <0x16>;
			vqmmc-supply = <0x16>;
			mmc-pwrseq = <0x18>;
			bus-width = <0x04>;
			non-removable;
			phandle = <0x52>;

			sdio_wifi@1 {
				reg = <0x01>;
				compatible = "brcm,bcm4329-fmac";
				interrupt-parent = <0x15>;
				interrupts = <0x06 0x0a 0x08>;
				interrupt-names = "host-wake";
				phandle = <0x53>;
			};
		};

		mmc@1c11000 {
			reg = <0x1c11000 0x1000>;
			resets = <0x03 0x09>;
			reset-names = "ahb";
			interrupts = <0x00 0x3e 0x04>;
			status = "disabled";
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			compatible = "allwinner,sun7i-a20-mmc";
			clocks = <0x03 0x18 0x03 0x4d 0x03 0x4f 0x03 0x4e>;
			clock-names = "ahb\0mmc\0output\0sample";
			phandle = <0x54>;
		};

		eeprom@1c14000 {
			reg = <0x1c14000 0x400>;
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			compatible = "allwinner,sun8i-h3-sid";
			phandle = <0x55>;

			thermal-sensor-calibration@34 {
				reg = <0x34 0x04>;
				phandle = <0x36>;
			};
		};

		mailbox@1c17000 {
			compatible = "allwinner,sun8i-h3-msgbox\0allwinner,sun6i-a31-msgbox";
			reg = <0x1c17000 0x1000>;
			clocks = <0x03 0x32>;
			resets = <0x03 0x24>;
			interrupts = <0x00 0x31 0x04>;
			#mbox-cells = <0x01>;
			phandle = <0x08>;
		};

		usb@1c19000 {
			compatible = "allwinner,sun8i-h3-musb";
			reg = <0x1c19000 0x400>;
			clocks = <0x03 0x20>;
			resets = <0x03 0x11>;
			interrupts = <0x00 0x47 0x04>;
			interrupt-names = "mc";
			phys = <0x19 0x00>;
			phy-names = "usb";
			extcon = <0x19 0x00>;
			dr_mode = "otg";
			status = "okay";
			phandle = <0x56>;
		};

		phy@1c19400 {
			compatible = "allwinner,sun8i-h3-usb-phy";
			reg = <0x1c19400 0x2c 0x1c1a800 0x04 0x1c1b800 0x04 0x1c1c800 0x04 0x1c1d800 0x04>;
			reg-names = "phy_ctrl\0pmu0\0pmu1\0pmu2\0pmu3";
			clocks = <0x03 0x58 0x03 0x59 0x03 0x5a 0x03 0x5b>;
			clock-names = "usb0_phy\0usb1_phy\0usb2_phy\0usb3_phy";
			resets = <0x03 0x00 0x03 0x01 0x03 0x02 0x03 0x03>;
			reset-names = "usb0_reset\0usb1_reset\0usb2_reset\0usb3_reset";
			status = "okay";
			#phy-cells = <0x01>;
			usb0_id_det-gpios = <0x15 0x06 0x0c 0x00>;
			usb0_vbus-supply = <0x1a>;
			phandle = <0x19>;
		};

		usb@1c1a000 {
			compatible = "allwinner,sun8i-h3-ehci\0generic-ehci";
			reg = <0x1c1a000 0x100>;
			interrupts = <0x00 0x48 0x04>;
			clocks = <0x03 0x21 0x03 0x25>;
			resets = <0x03 0x12 0x03 0x16>;
			phys = <0x19 0x00>;
			phy-names = "usb";
			status = "okay";
			phandle = <0x57>;
		};

		usb@1c1a400 {
			compatible = "allwinner,sun8i-h3-ohci\0generic-ohci";
			reg = <0x1c1a400 0x100>;
			interrupts = <0x00 0x49 0x04>;
			clocks = <0x03 0x21 0x03 0x25 0x03 0x5c>;
			resets = <0x03 0x12 0x03 0x16>;
			phys = <0x19 0x00>;
			phy-names = "usb";
			status = "okay";
			phandle = <0x58>;
		};

		usb@1c1b000 {
			compatible = "allwinner,sun8i-h3-ehci\0generic-ehci";
			reg = <0x1c1b000 0x100>;
			interrupts = <0x00 0x4a 0x04>;
			clocks = <0x03 0x22 0x03 0x26>;
			resets = <0x03 0x13 0x03 0x17>;
			phys = <0x19 0x01>;
			phy-names = "usb";
			status = "disabled";
			phandle = <0x59>;
		};

		usb@1c1b400 {
			compatible = "allwinner,sun8i-h3-ohci\0generic-ohci";
			reg = <0x1c1b400 0x100>;
			interrupts = <0x00 0x4b 0x04>;
			clocks = <0x03 0x22 0x03 0x26 0x03 0x5d>;
			resets = <0x03 0x13 0x03 0x17>;
			phys = <0x19 0x01>;
			phy-names = "usb";
			status = "disabled";
			phandle = <0x5a>;
		};

		usb@1c1c000 {
			compatible = "allwinner,sun8i-h3-ehci\0generic-ehci";
			reg = <0x1c1c000 0x100>;
			interrupts = <0x00 0x4c 0x04>;
			clocks = <0x03 0x23 0x03 0x27>;
			resets = <0x03 0x14 0x03 0x18>;
			phys = <0x19 0x02>;
			phy-names = "usb";
			status = "disabled";
			phandle = <0x5b>;
		};

		usb@1c1c400 {
			compatible = "allwinner,sun8i-h3-ohci\0generic-ohci";
			reg = <0x1c1c400 0x100>;
			interrupts = <0x00 0x4d 0x04>;
			clocks = <0x03 0x23 0x03 0x27 0x03 0x5e>;
			resets = <0x03 0x14 0x03 0x18>;
			phys = <0x19 0x02>;
			phy-names = "usb";
			status = "disabled";
			phandle = <0x5c>;
		};

		usb@1c1d000 {
			compatible = "allwinner,sun8i-h3-ehci\0generic-ehci";
			reg = <0x1c1d000 0x100>;
			interrupts = <0x00 0x4e 0x04>;
			clocks = <0x03 0x24 0x03 0x28>;
			resets = <0x03 0x15 0x03 0x19>;
			phys = <0x19 0x03>;
			phy-names = "usb";
			status = "disabled";
			phandle = <0x5d>;
		};

		usb@1c1d400 {
			compatible = "allwinner,sun8i-h3-ohci\0generic-ohci";
			reg = <0x1c1d400 0x100>;
			interrupts = <0x00 0x4f 0x04>;
			clocks = <0x03 0x24 0x03 0x28 0x03 0x5f>;
			resets = <0x03 0x15 0x03 0x19>;
			phys = <0x19 0x03>;
			phy-names = "usb";
			status = "disabled";
			phandle = <0x5e>;
		};

		clock@1c20000 {
			reg = <0x1c20000 0x400>;
			clocks = <0x1b 0x1c 0x00>;
			clock-names = "hosc\0losc";
			protected-clocks = <0x32>;
			#clock-cells = <0x01>;
			#reset-cells = <0x01>;
			compatible = "allwinner,sun8i-h3-ccu";
			phandle = <0x03>;
		};

		pinctrl@1c20800 {
			reg = <0x1c20800 0x400>;
			interrupt-parent = <0x1d>;
			interrupts = <0x00 0x0b 0x04 0x00 0x11 0x04>;
			clocks = <0x03 0x36 0x1b 0x1c 0x00>;
			clock-names = "apb\0hosc\0losc";
			gpio-controller;
			#gpio-cells = <0x03>;
			interrupt-controller;
			#interrupt-cells = <0x03>;
			compatible = "allwinner,sun8i-h3-pinctrl";
			phandle = <0x15>;

			csi-pins {
				pins = "PE0\0PE2\0PE3\0PE4\0PE5\0PE6\0PE7\0PE8\0PE9\0PE10\0PE11";
				function = "csi";
				phandle = <0x2b>;
			};

			emac-rgmii-pins {
				pins = "PD0\0PD1\0PD2\0PD3\0PD4\0PD5\0PD7\0PD8\0PD9\0PD10\0PD12\0PD13\0PD15\0PD16\0PD17";
				function = "emac";
				drive-strength = <0x28>;
				phandle = <0x5f>;
			};

			i2c0-pins {
				pins = "PA11\0PA12";
				function = "i2c0";
				phandle = <0x28>;
			};

			i2c1-pins {
				pins = "PA18\0PA19";
				function = "i2c1";
				phandle = <0x29>;
			};

			i2c2-pins {
				pins = "PE12\0PE13";
				function = "i2c2";
				phandle = <0x2a>;
			};

			i2s0-pins {
				pins = "PA18\0PA19\0PA20\0PA21";
				function = "i2s0";
				phandle = <0x60>;
			};

			i2s1-pins {
				pins = "PG10\0PG11\0PG12\0PG13";
				function = "i2s1";
				phandle = <0x61>;
			};

			mmc0-pins {
				pins = "PF0\0PF1\0PF2\0PF3\0PF4\0PF5";
				function = "mmc0";
				drive-strength = <0x1e>;
				bias-pull-up;
				phandle = <0x14>;
			};

			mmc1-pins {
				pins = "PG0\0PG1\0PG2\0PG3\0PG4\0PG5";
				function = "mmc1";
				drive-strength = <0x1e>;
				bias-pull-up;
				phandle = <0x17>;
			};

			mmc2-8bit-pins {
				pins = "PC5\0PC6\0PC8\0PC9\0PC10\0PC11\0PC12\0PC13\0PC14\0PC15\0PC16";
				function = "mmc2";
				drive-strength = <0x1e>;
				bias-pull-up;
				phandle = <0x62>;
			};

			spdif-tx-pin {
				pins = "PA17";
				function = "spdif";
				phandle = <0x63>;
			};

			spi0-pins {
				pins = "PC0\0PC1\0PC2\0PC3";
				function = "spi0";
				phandle = <0x22>;
			};

			spi1-pins {
				pins = "PA15\0PA16\0PA14\0PA13";
				function = "spi1";
				phandle = <0x23>;
			};

			uart0-pa-pins {
				pins = "PA4\0PA5";
				function = "uart0";
				phandle = <0x25>;
			};

			uart1-pins {
				pins = "PG6\0PG7";
				function = "uart1";
				phandle = <0x64>;
			};

			uart1-rts-cts-pins {
				pins = "PG8\0PG9";
				function = "uart1";
				phandle = <0x65>;
			};

			uart2-pins {
				pins = "PA0\0PA1";
				function = "uart2";
				phandle = <0x26>;
			};

			uart2-rts-cts-pins {
				pins = "PA2\0PA3";
				function = "uart2";
				phandle = <0x27>;
			};

			uart3-pins {
				pins = "PA13\0PA14";
				function = "uart3";
				phandle = <0x66>;
			};

			uart3-rts-cts-pins {
				pins = "PA15\0PA16";
				function = "uart3";
				phandle = <0x67>;
			};
		};

		timer@1c20c00 {
			compatible = "allwinner,sun8i-a23-timer";
			reg = <0x1c20c00 0xa0>;
			interrupts = <0x00 0x12 0x04 0x00 0x13 0x04>;
			clocks = <0x1b>;
		};

		ethernet@1c30000 {
			compatible = "allwinner,sun8i-h3-emac";
			syscon = <0x1e>;
			reg = <0x1c30000 0x10000>;
			interrupts = <0x00 0x52 0x04>;
			interrupt-names = "macirq";
			resets = <0x03 0x0c>;
			reset-names = "stmmaceth";
			clocks = <0x03 0x1b>;
			clock-names = "stmmaceth";
			status = "okay";
			phy-handle = <0x1f>;
			phy-mode = "mii";
			allwinner,leds-active-low;
			phandle = <0x68>;

			mdio {
				#address-cells = <0x01>;
				#size-cells = <0x00>;
				compatible = "snps,dwmac-mdio";
				phandle = <0x20>;
			};

			mdio-mux {
				compatible = "allwinner,sun8i-h3-mdio-mux";
				#address-cells = <0x01>;
				#size-cells = <0x00>;
				mdio-parent-bus = <0x20>;

				mdio@1 {
					compatible = "allwinner,sun8i-h3-mdio-internal";
