	thermal-zones {

		cpu_thermal {
			polling-delay-passive = <0xfa>;
			polling-delay = <0x3e8>;
			thermal-sensors = <0x3e>;

			trips {

				cpu_warm {
					temperature = <0x124f8>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x3f>;
				};

				cpu_hot_pre {
					temperature = <0x13880>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x40>;
				};

				cpu_hot {
					temperature = <0x14c08>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x41>;
				};

				cpu_very_hot_pre {
					temperature = <0x15f90>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x42>;
				};

				cpu_very_hot {
					temperature = <0x17318>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x43>;
				};

				cpu_crit {
					temperature = <0x19a28>;
					hysteresis = <0x7d0>;
					type = "critical";
					phandle = <0x88>;
				};
			};

			cooling-maps {

				cpu_warm_limit_cpu {
					trip = <0x3f>;
					cooling-device = <0x3a 0xffffffff 0x02>;
				};

				cpu_hot_pre_limit_cpu {
					trip = <0x40>;
					cooling-device = <0x3a 0x02 0x03>;
				};

				cpu_hot_limit_cpu {
					trip = <0x41>;
					cooling-device = <0x3a 0x03 0x04>;
				};

				cpu_very_hot_pre_limit_cpu {
					trip = <0x42>;
					cooling-device = <0x3a 0x05 0x06>;
				};

				cpu_very_hot_limit_cpu {
					trip = <0x43>;
					cooling-device = <0x3a 0x07 0xffffffff>;
				};
			};
		};
	};

